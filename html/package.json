{
  "name": "sample-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl --ssl-key ssl/localhost.key  --ssl-cert ssl/localhost.crt --proxy-config=proxy.conf.js",
    "build": "ng build --configuration production",
    "test": "ng test sample-app --watch=false --browsers=ChromeHeadlessNoSandbox",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:app": "ng serve sample-app --proxy-config=proxy.conf.js --ssl --ssl-key ssl/localhost.key  --ssl-cert ssl/localhost.crt",
    "test:app": "ng test",
    "check:app": "npm run lint && npm run test && npm run build",
    "concat:mocks": "node \"./mocks/concat-json.js\"",
    "mock:server": "npm run concat:mocks && json-server --routes mocks/routes.json --watch mocks/data.json",
    "zip": "bestzip ./sample-app.zip dist/*",
    "compodoc": "npx compodoc -p tsconfig.doc.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.1.3",
    "@angular/cdk": "16.1.3",
    "@angular/common": "16.1.3",
    "@angular/compiler": "16.1.3",
    "@angular/core": "16.1.3",
    "@angular/forms": "16.1.3",
    "@angular/localize": "16.1.3",
    "@angular/platform-browser": "16.1.3",
    "@angular/platform-browser-dynamic": "16.1.3",
    "@angular/router": "16.1.3",
    "@angular/service-worker": "16.1.3",
    "@fortawesome/angular-fontawesome": "0.13.0",
    "@fortawesome/fontawesome-svg-core": "6.4.0",
    "@fortawesome/free-solid-svg-icons": "6.4.0",
    "@ng-bootstrap/ng-bootstrap": "15.0.1",
    "@ng-select/ng-select": "11.0.0",
    "@ngx-translate/core": "15.0.0",
    "@ngx-translate/http-loader": "8.0.0",
    "@popperjs/core": "2.11.8",
    "@swimlane/ngx-datatable": "20.1.0",
    "bootstrap": "5.3.0",
    "core-js": "3.31.0",
    "moment": "2.29.4",
    "ng-recaptcha": "12.0.1",
    "ngx-spinner": "16.0.2",
    "ngx-toastr": "17.0.2",
